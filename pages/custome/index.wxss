/* pages/custome/index.wxss */

@font-face {
  font-family: "iconfont";
  src: url('iconfont.eot?t=1540956036290'); /* IE9*/
  src: url('iconfont.eot?t=1540956036290#iefix') format('embedded-opentype'),
         /* IE6-IE8 */ url('data:application/x-font-woff;charset=utf-8;base64,') format('woff'),
         url('iconfont.ttf?t=1540956036290') format('truetype'),
         /* chrome,
         firefox,
         opera,
         Safari,
         Android,
         iOS 4.2+*/ url('iconfont.svg?t=1540956036290#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-dianhua:before {
  content: "\e68a";
}

.icon-time:before {
  content: "\e604";
}

.icon-xiasanjiao:before {
  content: "\e6be";
}

page {
  padding: 10rpx;
  background: #f5f5f5;
}

.top {
  display: flex;
  background: #fff;
  padding: 15rpx 10rpx;
  width: 95.5%;
}

.top text {
  flex: 1;
  text-align: center;
}

.top text:first-child {
  text-align: left;
}

.top text:last-child {
  text-align: right;
}

.top text text {
  color: #b6b6b6;
}

.list {
  background: #fff;
  padding: 15rpx;
  margin-top: 15rpx;
  width: 94%;
}

.list > view {
  overflow: hidden;
}

.list > view .left:first-child {
  width: 150rpx;
  height: 150rpx;
  border-radius: 100%;
}

.sale {
  padding: 20rpx 0rpx;
}

.sale text {
  font-size: 26rpx;
  padding: 5rpx 10rpx;
  border-radius: 5rpx;
  margin-right: 10rpx;
}

.sale text:first-child {
  color: #e40c01;
  background: rgba(255, 240, 184, 0.5);
}

.sale text:nth-child(2) {
  color: #1c87d8;
  background: rgba(191, 220, 242, 0.5);
}

.sale text:last-child {
  color: #5cb66a;
  background: rgba(210, 236, 180, 0.5);
}

.left {
  font-size: 32rpx;
  margin-right: 10rpx;
  color: #323232;
}

.left > text {
  color: #666;
}

.list > view .right {
  width: 60rpx;
  height: 45rpx;
  margin-left: 8%;
}

.list > view:last-child {
  border-top: 1px solid #eee;
  padding-top: 15rpx;
  margin-top: 35rpx;
}

.list > view:last-child text {
  color: #ddd;
}

.list > view:last-child .d {
  color: #999;
  font-size: 26rpx;
  margin-left: 10rpx;
}

.bottom {
  position: fixed;
  bottom: 0rpx;
  width: 100%;
  padding: 20rpx 10rpx;
}

.bottom > view {
  text-align: center;
  background: #fff;
  padding: 50rpx 20rpx;
  width: 90%;
}

.bottom image {
  height: 2rpx;
  margin-top: -5rpx;
}

.bottom > view view:first-child {
  margin-bottom: -20rpx;
  font-size: 36rpx;
  font-weight: bold;
  color: #666;
}

.bottom > view view {
  font-size: 32rpx;
  color: #7c7c7c;
}

.fixed {
  height: 100%;
  top: 0rpx;
  left: 0rpx;
}

.fixed > view {
  position: absolute;
  bottom: 0rpx;
  background: #fff;
  height: 45%;
  width: 100%;
}

.fixed .type {
  overflow: hidden;
  overflow-y: scroll;
}

.type view {
  text-align: center;
  padding: 25rpx;
  border-bottom: 1px solid #dcdcdc;
  margin: 0rpx 20rpx;
}

.fixed button {
  background: #1d87d9;
  width: 95%;
  color: #fff;
  box-shadow: 0px 1px 1px 0px #1d87d9;
  position: absolute;
  bottom: 10rpx;
  left: 2.5%;
}

.type .active {
  color: #1d87d9;
}

.fixed .san {
  width: 50rpx;
  height: 22rpx;
  position: absolute;
  top: -19rpx;
}
