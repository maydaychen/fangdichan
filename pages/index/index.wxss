.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-cx:before {
  content: "\e600";
}

input {
  display: inline-block;
  width: 500rpx;
  vertical-align: middle;
  margin-left: 10rpx;
}

.search {
  width: 717rpx;
  height: 72rpx;
  margin: 0 auto;
  margin-top: 19rpx;
  margin-bottom: 13rpx;
  padding-left: 30rpx;
  box-sizing: border-box;
  border-radius: 5rpx;
  display: flex;
  align-items: center;
  background: var(--color-white);
}

.search text {
  font-size: var(--font-size-medium-x);
  vertical-align: middle;
}

.search .search-input {
  font-size: var(--font-size-medium-x);
  color: var(--color-placeholder-color);
  margin-left: 20rpx;
}

.item {
  width: 717rpx;
  height: 165rpx;
  margin: 0 auto;
  margin-bottom: 10rpx;
  background-color: var(--color-white);
  border-radius: 5rpx;
  padding: 22rpx 30rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.item .left {
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
}

.item .left .avatar {
  width: 103rpx;
  height: 103rpx;
  margin-right: 20rpx;
  border-radius: 50%;
}

.avatar image {
  width: 100%;
  height: 100%;
  border-radius: 50%;
}

.left .content {
  height: 75%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.content .name {
  color: var(--color-certification-title);
  font-size: var(--font-size-large-y);
}

.content .message {
  color: var(--color-sub-title);
  font-size: var(--font-size-medium);
}

.item .right {
  height: 100%;
  display: flex;
  align-items: center;
}

.time-icon {
  width: 26rpx;
  height: 26rpx;
  margin-right: 6rpx;
}

.time-icon image {
  width: 100%;
  height: 100%;
  display: block;
}

.right .time {
  font-size: var(--font-size-medium);
  color: var(--color-placeholder-color);
}