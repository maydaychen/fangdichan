/* pages/house/rent/zhengzu/new/index.wxss */
@font-face {
  font-family: "iconfont";
  src: url('iconfont.eot?t=1542181585629');
  /* IE9*/
  src: url('iconfont.eot?t=1542181585629#iefix') format('embedded-opentype'),
    /* IE6-IE8 */
    url('data:application/x-font-woff;charset=utf-8;base64,') format('woff'),
    url('iconfont.ttf?t=1542181585629') format('truetype'),
    /* chrome, firefox, opera, Safari, Android, iOS 4.2+*/
    url('iconfont.svg?t=1542181585629#iconfont') format('svg');
  /* iOS 4.1- */
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-LC_icon_close_fill_circle:before {
  content: "\e605";
}

.icon-search:before {
  content: "\e6a4";
}

.icon-xiangji:before {
  content: "\e6b5";
}

.icon-right:before {
  content: "\e7eb";
}

.icon-yinlianglabashengyin:before {
  content: "\e7b5";
}

page {
  height: 100%;
}

input {
  display: inline-block;
  vertical-align: middle;
  width: 130rpx;
  background: #f5f5f5;
  border-radius: 10rpx;
  margin: 0 20rpx;
  text-align: center;
  padding-left: 10rpx;
}

.container {
  padding: 15rpx;
}

.top {
  background: #f2f8fd;
  padding: 10rpx;
  position: relative;
  color: #999;
  font-size: 26rpx;
  border:2rpx solid rgba(162, 213, 252, 0.2);
}

.top text:first-child {
  color: #707070;
}

.top text:last-child {
  color: #bfbfbf;
  position: absolute;
  right: -5rpx;
  top: -5rpx;
  font-size: 24rpx;
}

.list>view {
  background: #fff;
  overflow: hidden;
  padding: 20rpx;
  font-size: 29rpx;
}

.view {
  border-bottom: 1px solid #ddd;
}

.tops {
  border-radius: 10rpx 10rpx 0 0;
  margin-top: 15rpx;
}

.last {
  border-radius: 0 0 10rpx 10rpx;
  box-shadow: 0px 3px 8px 0px rgba(172, 171, 171, 0.15);
}

.message {
  color: #666;
  font-size: 25rpx;
  margin-bottom: 15rpx;
}

.left {
  width: 33.3333%;
  height: 215rpx;
  text-align: center;
}

view.left {
  background: #f5f5f5;
}

.left text {
  font-size: 100rpx;
  color: #999;
  line-height: 215rpx;
}

.tops checkbox {
  margin-top: -9%;
}

.xieyi {
  color: #CE2323;
  font-size: 27rpx;
  text-decoration: underline;
}

button {
  background: #1d87d9;
  width: 95%;
  color: #fff;
  box-shadow: 0px 1px 1px 0px #1d87d9;
  margin: 0 auto;
  margin-top: 20rpx;
}

/* 隐藏的部分 */

.fixed {
  height: 100%;
  z-index: 10;
  left: 0rpx;
}

.fixed>view {
  background: #fff;
  position: absolute;
  bottom: 0rpx;
  width: 100%;
  text-align: center;
  padding-bottom: 130rpx;
}

.fixed .bottom {
  padding: 20rpx;
  font-size: var(--font-size-large);
  color: var(--color-certification-title);
  border-bottom: 1px solid #DCDCDC;
}

.fixed>button {
  position: absolute;
  bottom: 20rpx;
  left: 2.5%;
}

.center {
  text-align: center;
  padding: 20rpx 0;
  border-bottom: 1rpx solid var(--color-border-color);
}

.search {
  padding: 20rpx;
}

.search>text {
  font-size: 60rpx;
  color: #999;
  vertical-align: middle;
  margin-left: 5%;
}

.search view:first-child {
  padding: 15rpx 30rpx;
  display: inline-block;
  background: #f5f5f5;
}


.search view input {
  width: 400rpx;
}

.search text {
  font-size: 35rpx;
  vertical-align: middle;
  color: #777;
}

.search>text {
  color: #D89622;
  font-size: 33rpx;
}

textarea {
  height: 226rpx;
  background: #f5f5f5;
  margin: 40rpx auto 20rpx;
  padding: 30rpx 40rpx;
  text-align: left;
  border: 1px solid #ddd;
  border-radius: 8rpx;
}

.textarea-placeholder {
  color: var(--color-sub-title);
  font-size: var(--font-size-medium-x);
}

.scroll {
  display: flex;
}

.scroll scroll-view {
  flex: 1;
}

.scroll scroll-view:first-child {
  background: #E5E5E5;
}

.scroll scroll-view:nth-child(2) {
  background: #F1F1F1;
}

.scroll text {
  display: block;
  text-align: center;
  border-bottom: 1px solid #ddd;
  padding: 20rpx;
}

.fixed>view radio-group {
  padding: 150rpx 0rpx;
}

.fixed view radio {
  margin-right: 10rpx;
}

.fixed view label {
  margin-right: 50rpx;
}

.fixed view label:last-child {
  margin-right: 0rpx;
}

.fixed .see {
  margin: 10%;
  width: 80%;
  border-radius: 10rpx;
  top: 0rpx;
}

.fixed .see text {
  position: absolute;
  right: -10rpx;
  top: -10rpx;
}

.choose-active {
  color: #1D87D9 !important;
}