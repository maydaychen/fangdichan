.icon-LC_icon_close_fill_circle:before { content: "\e605"; }

.icon-search:before { content: "\e6a4"; }

.icon-right:before { content: "\e7eb"; }

.icon-yinlianglabashengyin:before { content: "\e7b5"; }

page {
  height:100%;
}

input {
  display: inline-block;
  vertical-align: middle;
  width: 130rpx;
  background: #f5f5f5;
  border-radius: 10rpx;
  margin: 0 10rpx;
}

.container {
  padding: 15rpx;
}

.top {
  background: #f2f8fd;
  padding: 20rpx;
  position: relative;
  color: #999;
  font-size: 26rpx;
}

.top text:first-child {
  color: #707070;
}

.top text:last-child {
  color: #bfbfbf;
  position: absolute;
  right: -5rpx;
  top: -5rpx;
}

.list > view {
  background: #fff;
  overflow: hidden;
  padding: 20rpx;
  font-size: 29rpx;
}

.view {
  border-bottom: 1px solid #ddd;
}

.tops {
  border-radius: 10rpx 10rpx 0 0;
  margin-top: 15rpx;
}

.last {
  border-radius: 0 0 10rpx 10rpx;
}

.message {
  color: #666;
  font-size: 25rpx;
  margin-bottom: 15rpx;
}

.left {
  width: 33.3333%;
  height: 215rpx;
  background: #f5f5f5;
  text-align: center;
}

.left text {
  font-size: 100rpx;
  color: #999;
  line-height: 215rpx;
}

.tops checkbox {
  margin-top: -9%;
}
.message text{
  color:#CE2323;
  font-size:27rpx;
  text-decoration:underline;
}
button {
  background: #1d87d9;
  width: 95%;
  color: #fff;
  box-shadow: 0px 1px 1px 0px #1d87d9;
  margin:0 auto;
  margin-top:20rpx;
}
/* 隐藏的部分 */

.fixed{
  height:100%;
  z-index:10;
  left:0rpx;
}
.fixed > view{
  background:#fff;
height:55%;
position:absolute;
bottom:0rpx;
width:100%;
}
.center{
  text-align:center;
}

.search {
  padding: 25rpx;
}

.search > text {
  font-size: 60rpx;
  color: #999;
  vertical-align: middle;
  margin-left: 5%;
}

.search view {
  padding: 15rpx 30rpx;
  display: inline-block;
  background: #f5f5f5;
}
.search view input{
  width:460rpx;
}
.search text {
  font-size: 35rpx;
  vertical-align:middle;
  color:#777;
  font-weight:bold;
}
textarea{
  background:#f5f5f5;
margin:0 auto;
margin-top:20rpx;
border:1px solid #ddd;
border-radius:8rpx;
}