.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-laba:before {
  content: "\e61d";
}

.icon-close:before {
  content: "\e600";
}

.icon-xiangji:before {
  content: "\e61b";
}

.icon-right:before {
  content: "\e63b";
}

page {
  background: #f5f5f5;
}

input {
  display: inline-block;
  vertical-align: middle;
  width: 130rpx;
  background: #f5f5f5;
  text-align: center;
  border-radius: 10rpx;
  margin: 0 10rpx;
}

.container {
  padding: 15rpx;
}

.top {
  background: #f2f8fd;
  padding: 20rpx;
  position: relative;
  color: #999;
  font-size: 26rpx;
}

.top text:first-child {
  color: #707070;
}

.top text:last-child {
  color: #bfbfbf;
  position: absolute;
  right: -5rpx;
  top: -5rpx;
}

.list > view {
  background: #fff;
  overflow: hidden;
  padding: 20rpx;
  font-size: 29rpx;
}

.view {
  border-bottom: 1px solid #ddd;
}

.tops {
  border-radius: 10rpx 10rpx 0 0;
  margin-top: 15rpx;
}

.last {
  border-radius: 0 0 10rpx 10rpx;
}

.message {
  color: #666;
  font-size: 25rpx;
  margin-bottom: 15rpx;
}

.left {
  width: 33.3333%;
  height: 215rpx;
  background: #f5f5f5;
  text-align: center;
}

.left text {
  font-size: 100rpx;
  color: #999;
  line-height: 215rpx;
}

.tops checkbox {
  margin-top: -9%;
}
.message text{
  color:#CE2323;
  font-size:27rpx;
  text-decoration:underline;
}
button {
  background: #1d87d9;
  width: 95%;
  color: #fff;
  box-shadow: 0px 1px 1px 0px #1d87d9;
  margin:0 auto;
  margin-top:20rpx;
}
