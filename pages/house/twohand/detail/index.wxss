page{
  background:#fff;
  height:100%;
}
.sale{padding:40rpx;text-align:center;}
.sale text{padding:5rpx 10rpx;border-radius:5rpx;margin-right:30rpx;} 
.sale text:first-child{color:#E40C01;background:#fff0b8;}
.sale text:nth-child(2){color:#1C87D8;background:#BFDCF2;}
.sale text:last-child{color:#5CB66A;background:#d2ecb4;}
.message{padding:15rpx;}
.message > text{width:45rpx;height:45rpx;border-radius:50%;color:#fff;background:#429EE1;padding:12rpx;margin-right:20rpx;}
.a > text{background:#F9C97A;font-size:36rpx;padding:9rpx;}
.b > text{background:#A9D891;font-size:39rpx;padding:7rpx;}
.message > view{margin-left:9%;font-size:28rpx;margin-right:15rpx;}
.message > view:first-child{display:flex;}
.message > view span{flex:1;color:#999;}
.message > view span:first-child{flex:1.5;}
.message > view span text{color:#000;}
.message .blue{color:#52A5E3;}