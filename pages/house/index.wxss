/* pages/house/index.wxss */

@font-face {
  font-family: "iconfont";
  src: url('iconfont.eot?t=1541055754239'); /* IE9*/
  src: url('iconfont.eot?t=1541055754239#iefix') format('embedded-opentype'),
         /* IE6-IE8 */ url('data:application/x-font-woff;charset=utf-8;base64,') format('woff'),
         url('iconfont.ttf?t=1541055754239') format('truetype'),
         /* chrome,
         firefox,
         opera,
         Safari,
         Android,
         iOS 4.2+*/ url('iconfont.svg?t=1541055754239#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-fangyuanguanli:before {
  content: "\e619";
}

.icon-fangyuanguanli_chuzu:before {
  content: "\e744";
}

.icon-right:before {
  content: "\e63b";
}

page {
  background: #f5f5f5;
}

.house_index {
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 3px 8px 0px rgba(172, 171, 171, 0.15);
  border-radius: 10rpx;
  display: flex;
  margin: 25rpx;
  width: 88%;
  padding: 20rpx;
  overflow: hidden;
}

.house_index > text {
  font-size: 60rpx;
  color: #fff;
  background: #13e6cd;
  border-radius: 100%;
  width: 112rpx;
  height: 102rpx;
  text-align: center;
  line-height: 100rpx;
}

.house_index > view {
  width: 88%;
  padding: 0rpx 20rpx;
}

.house_index .two {
  background: #1386d9;
}

.house_index image {
  width: 100rpx;
  height: 100rpx;
}

.house_title {
  width: 168px;
  height: 33px;
  font-size: 37rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(50, 50, 50, 1);
  line-height: 24px;
}

.house_sub {
  width: 170rpx;
  font-size: 25rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: #666;
  line-height: 24px;
  background: #eee;
  text-align: center;
  border-radius: 5rpx;
}

.right .right {
  margin-top: -15%;
  font-size: 32rpx;
  font-weight: bold;
}
